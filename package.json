{
  "name": "windmill-dashboard-react",
  "version": "0.4.0",
  "description": "A multi theme, completely accessible, with components and pages examples, (almost) ready for production dashboard.",
  "private": true,
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tailwind:build": "cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "tailwind:dev": "tailwindcss build src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@tinymce/tinymce-react": "^6.3.0",
    "@windmill/react-ui": "0.3.1",
    "axios": "^1.8.4",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "faker": "4.1.0",
    "html2pdf.js": "^0.9.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "2.9.0",
    "react-datepicker": "^8.3.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "react-focus-lock": "2.4.0",
    "react-hook-form": "^7.56.1",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.10.1",
    "react-transition-group": "4.4.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.26.9",
    "@release-it/conventional-changelog": "1.1.4",
    "@svgr/webpack": "5.4.0",
    "autoprefixer": "9.8.0",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "postcss-cli": "7.1.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "react-axe": "3.5.2",
    "release-it": "13.6.4",
    "tailwindcss": "1.4.6"
  },
  "keywords": [
    "windmill",
    "dashboard",
    "admin",
    "tailwind",
    "react"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
